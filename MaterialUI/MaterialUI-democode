import './App.css';
import TextField from '@mui/material/TextField';
import Button from '@mui/material/Button';
import SendIcon from '@mui/icons-material/Send';
import DeleteIcon from '@mui/icons-material/Delete';
import Radio from '@mui/material/Radio';
import RadioGroup from '@mui/material/RadioGroup';
import FormControlLabel from '@mui/material/FormControlLabel';

function App() {
  return (
    <div className="App">
      <TextField variant="filled" color="secondary" type="date" />
      <br />
      <Button
        onClick={() => alert('Clicked me')}
        startIcon={<SendIcon />}
        variant="contained"
        size="large"
        color="primary"
      >
        Click here
      </Button>

      <Button
        onClick={() => alert('Clicked me')}
        endIcon={<DeleteIcon />}
        variant="contained"
        size="large"
        color="secondary"
      >
        Click here
      </Button>

      <RadioGroup>
        <FormControlLabel control={<Radio />} value="Check1" label="Check1" />
        <FormControlLabel control={<Radio />} value="Check2" label="Check2" />
      </RadioGroup>
    </div>
  );
}

export default App;
